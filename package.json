{
  "name": "ReactReduxTemplate",
  "version": "1.0.0",
  "description": "React+ReduxTemplate",
  "main": "app/main.jsx",
  "scripts": {
    "build": "NODE_ENV=production webpack --config configs/webpack.config.prod.js --progress --profile --colors",
    "clean": "rimraf build dist coverage",
    "coverage": "./node_modules/jest/bin/jest.js --coverage",
    "lint": "eslint app/** --cache",
    "lint-no-cache": "eslint 'app/**'",
    "start": "node configs/devServer.js",
    "test": "./node_modules/jest/bin/jest.js",
    "test-dev": "./node_modules/jest/bin/jest.js --watch"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "moduleNameMapper": {
      "^rk(.*)$": "<rootDir>/app$1"
    },
    "notify": true,
    "setupTestFrameworkScriptFile": "./configs/setup-jasmine-env.js",
    "testRegex": "__tests__/.*.js$|__tests__/.*.jsx$"
  },
  "author": "Ryan Kim",
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.0",
    "babel-jest": "^17.0.2",
    "babel-loader": "^6.2.7",
    "babel-plugin-lodash": "^3.2.9",
    "babel-plugin-react-require": "^3.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "css-loader": "^0.25.0",
    "eslint": "^3.10.2",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-react": "^6.7.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "fetch-mock": "^5.5.0",
    "file-loader": "^0.9.0",
    "font-awesome": "^4.7.0",
    "jasmine-reporters": "^2.2.0",
    "jest": "^17.0.2",
    "node-sass": "^3.13.0",
    "postcss-loader": "^1.1.1",
    "react-addons-test-utils": "^15.4.0",
    "react-test-renderer": "^15.4.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "webpack": "^1.13.3",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.2",
    "xhr-mock": "^1.6.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "es6-promise": "^4.0.5",
    "lodash": "^4.16.4",
    "react": "^15.4.0",
    "react-addons-css-transition-group": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-redux": "^4.4.5",
    "react-redux-popup": "ryank109/react-redux-popup#0.7.0",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.4",
    "whatwg-fetch": "^2.0.0"
  }
}
